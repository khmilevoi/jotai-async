{
  "name": "jotai-async",
  "version": "0.3.1",
  "description": "Async side effects for jotai",
  "private": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.mts",
        "default": "./dist/*/index.mjs"
      },
      "default": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khmilevoi/jotai-async.git"
  },
  "keywords": [
    "jotai",
    "jotai-async",
    "react",
    "jotai-effect"
  ],
  "author": "khmilevoi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khmilevoi/jotai-async/issues"
  },
  "homepage": "https://github.com/khmilevoi/jotai-async#readme",
  "dependencies": {
    "jotai-effect": "^1.0.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-swc": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@types/react": "^18.3.10",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.11.1",
    "jotai": "^2.10.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "rollup": "^4.22.5",
    "rollup-plugin-dts": "^6.1.1"
  },
  "peerDependencies": {
    "jotai": "^2.10.0",
    "react": "^18.3.1",
    "typescript": "^5.6.2"
  }
}
